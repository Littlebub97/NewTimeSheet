@*

*@
@{
    ViewData["Title"] = "ClockIn";

}
<h1>@ViewData["Title"]</h1>
<h3>@ViewData["Message"]</h3>
<form>
    <h4>Clock</h4><button type="button">Clock In!</button>
    <img src="~/images/Clock-icon.png" />
    Time: <input type="datetime" name="Time" /><br />
    <p id="demo"></p>
    <script>
        var d = new Date();
        document.getElementById("demo").innerHTML = d;
    </script>
    <style>
        body {
            background-color: aquamarine;
        }
    </style>


</form>